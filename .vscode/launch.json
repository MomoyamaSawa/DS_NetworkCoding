{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "client.Main"
        },
        {
            "type": "java",
            "name": "GatewayApplication",
            "request": "launch",
            "mainClass": "gateway.geteway.GatewayApplication"
        },
        {
            "type": "java",
            "name": "DataServiceApplication",
            "request": "launch",
            "mainClass": "service.data.dataservice.DataServiceApplication",
            "projectName": "data-service",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=8141 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=data-service"
        },
        {
            "name": "Python: 当前文件",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "type": "java",
            "name": "Spring Boot-InfoServiceApplication<info-service>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "service.info.infoservice.InfoServiceApplication",
            "projectName": "info-service",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=8217 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=info-service"
        },
        {
            "type": "java",
            "name": "Spring Boot-DataServiceApplication<dataservice>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "service.data.dataservice.DataServiceApplication",
            "projectName": "dataservice",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-InfoServiceApplication<infoservice>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "service.info.infoservice.InfoServiceApplication",
            "projectName": "infoservice",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-GatewayApplication<gateway>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "gateway.geteway.GatewayApplication",
            "projectName": "gateway",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
