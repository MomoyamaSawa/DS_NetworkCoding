{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "cwd": "${workspaceFolder}/q3/src/demo",
            "mainClass": "test.client.Main"
        },
        {
            "type": "java",
            "name": "DATA-0",
            "request": "launch",
            "mainClass": "service.data.dataservice.DataServiceApplication",
            "cwd": "${workspaceFolder}/q3/src/data-service",
            "env": {
                "server.port": "8082"
            }
        },
        {
            "type": "java",
            "name": "DATA-1",
            "request": "launch",
            "mainClass": "service.data.dataservice.DataServiceApplication",
            "cwd": "${workspaceFolder}/q3/src/data-service",
            "env": {
                "server.port": "8083"
            }
        },
        {
            "type": "java",
            "name": "DATA-2",
            "request": "launch",
            "mainClass": "service.data.dataservice.DataServiceApplication",
            "cwd": "${workspaceFolder}/q3/src/data-service",
            "env": {
                "server.port": "8084"
            }
        },
        {
            "type": "java",
            "name": "INFO-0",
            "request": "launch",
            "mainClass": "service.info.infoservice.InfoServiceApplication",
            "cwd": "${workspaceFolder}/q3/src/info-service"
        },
        {
            "type": "java",
            "name": "Gateway",
            "request": "launch",
            "mainClass": "gateway.geteway.GatewayApplication"
        },
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "c++: g++",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "D:\\Tools\\GCC\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "g++"
        }
    ]
}